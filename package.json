{
  "name": "@binkylabs/kiota-typespec-emitter",
  "version": "0.1.0",
  "description": "A client code emitter for TypeSpec that uses kiota to generate the code to call REST APIs",
  "keywords": [
    "TypeSpec",
    "tooling",
    "kiota",
    "REST",
    "OpenAPI",
    "C#",
    "Java",
    "TyeScript",
    "Go",
    "PHP",
    "Python"
  ],
  "homepage": "https://github.com/BinkyLabs/kiota-typespec-emitter#readme",
  "bugs": {
    "url": "https://github.com/BinkyLabs/kiota-typespec-emitter/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BinkyLabs/kiota-typespec-emitter.git"
  },
  "author": "Vincent Biret",
  "engines": {
    "node": ">=22.0.0"
  },
  "type": "module",
  "main": "dist/src/index.js",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./testing": {
      "types": "./dist/src/testing/index.d.ts",
      "default": "./dist/src/testing/index.js"
    }
  },
  "peerDependencies": {
    "@typespec/compiler": "latest"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@typespec/compiler": "latest",
    "@typespec/http": "^1.8.0",
    "eslint": "^9.15.0",
    "prettier": "^3.3.3",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.54.0"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "node --test \"dist/test/**/*.test.js\"",
    "lint:check": "eslint src/ test/ --report-unused-disable-directives --max-warnings=0",
    "lint": "eslint . --report-unused-disable-directives --fix",
    "format": "prettier .  --write",
    "format:check": "prettier --check ."
  },
  "private": false,
  "packageManager": "npm@11.8.0+sha512.9f5f6c25e5be44629d907a3c482739c614b090a8505057d9685cd273e12c6172e34aa215d2d97bd9a0d8415bb356f7eb6f2b06c1d690b0d2cdcb9f09d7410149",
  "dependencies": {
    "@microsoft/kiota": "^1.30.0",
    "@typespec/openapi3": "^1.8.0",
    "adm-zip": "^0.5.16",
    "original-fs": "^1.2.0",
    "uuid": "^13.0.0",
    "vscode-jsonrpc": "^9.0.0-next.11"
  }
}
